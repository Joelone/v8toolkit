

export BUILD_COMMAND="time clang++ -g0 -I${V8_INCLUDES} compilation_timing_sample.cpp -I../include -std=c++14 -c"


echo ${BUILD_COMMAND} -DV8HELPERS
${BUILD_COMMAND} -DV8HELPERS
echo
echo ${BUILD_COMMAND} -DV8TOOLKIT
${BUILD_COMMAND} -DV8TOOLKIT
echo
echo ${BUILD_COMMAND} -DV8_CLASS_WRAPPER
${BUILD_COMMAND} -DV8_CLASS_WRAPPER
echo
echo ${BUILD_COMMAND} -DJAVASCRIPT
${BUILD_COMMAND} -DJAVASCRIPT
echo
echo ${BUILD_COMMAND} -DSTD_FUNCTIONS
${BUILD_COMMAND} -DSTD_FUNCTIONS
echo
echo ${BUILD_COMMAND} -DCREATE_CONTEXT
${BUILD_COMMAND} -DCREATE_CONTEXT
echo
echo ${BUILD_COMMAND} -DWRAP_EMPTY_CLASS
${BUILD_COMMAND} -DWRAP_EMPTY_CLASS
echo
echo ${BUILD_COMMAND} -DWRAP_DATA_MEMBER_CLASS
${BUILD_COMMAND} -DWRAP_DATA_MEMBER_CLASS
echo
echo ${BUILD_COMMAND} -DWRAP_5_INT_DATA_MEMBERS_CLASS
${BUILD_COMMAND} -DWRAP_5_INT_DATA_MEMBERS_CLASS
echo
echo ${BUILD_COMMAND} -DWRAP_5_DIFFERENT_DATA_MEMBERS_CLASS
${BUILD_COMMAND} -DWRAP_5_DIFFERENT_DATA_MEMBERS_CLASS
echo

# Same as above but two different classes with identical contents
echo ${BUILD_COMMAND} -DWRAP_5_DIFFERENT_DATA_MEMBERS_2_CLASSES
${BUILD_COMMAND} -DWRAP_5_DIFFERENT_DATA_MEMBERS_2_CLASSES
echo




